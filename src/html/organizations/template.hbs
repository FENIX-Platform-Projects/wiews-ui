        </div>
        <div class="row spacer">
            <!-- Filters -->
            <div class="col-md-12">
                <div class="form-inline">
                    <input type="text" class="form-control" id="omnibox"
                           placeholder="Type here your search by Name, Organization acronym, WIEWS instcode, City or Country">
                    <div class="pull-right">
                        <button style="width: 55px" id="search" type="button" class="btn btn-primary"><span class="icojam icojam_search"></span></button>
                        <!--
                        <button  type="button" class="btn btn-default">Switch to Advanced Search</button>
                        -->
                    </div>
                </div>
            </div>
        </div>
        <div id="advanced-search" class="advanced">
            <hr>
            <br/>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput1">Name of organization</label>
                        <input type="text" class="form-control" id="exampleInput1" placeholder="Name of organization">
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput2">Organization acronym</label>
                        <input type="text" class="form-control" id="exampleInput2" placeholder="Organization acronym">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput3">WIEWS instcode</label>
                        <input type="text" class="form-control" id="exampleInput3" placeholder="WIEWS instcode">
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput4">City</label>
                        <input type="text" class="form-control" id="exampleInput4" placeholder="City">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput7">Country</label>
                        <select id="exampleInput7" class="form-control">
                            <option></option>
                            <option>Spain</option>
                            <option>Italy</option>
                            <option>Germany</option>
                            <option>Panama</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput6">Organization role categories</label>
                        <select id="exampleInput6" id="exampleInput6" class="form-control"
                                placeholder="Organization role categories">
                            <option></option>
                            <option>Genebank (long term collections)</option>
                            <option>Genebank (medium term collections)</option>
                            <option>Genebank (short term collections)</option>
                            <option>Botanical garden</option>
                            <option>Breeder</option>
                            <option>Network</option>
                            <option>Community</option>
                            <option>Educational</option>
                            <option>Seed producer</option>
                            <option>Seed supplier</option>
                            <option>Farmer community</option>
                            <option>Research</option>
                            <option>Extensionist</option>
                            <option>Laboratory</option>
                            <option>Publisher</option>
                            <option>Administration/Policy</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="exampleInput9">Valid/invalidated organizations</label>
                        <select id="exampleInput9" class="form-control">
                            <option selected>Valid only</option>
                            <option>Valid and invalidated</option>
                            <option>Invalidated only</option>
                        </select>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="pull-right">
                        <div style="margin-top: 24px">
                            <button id="adv_search" type="button" class="btn btn-primary"><span class="icojam icojam_search"></span></button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <div class="row spacer"><!-- Spacer --></div>
    </div>
    <div data-role='results'>
        <div class="row">
            <div class="col-md-6">
                <h2>Results</h2></br>
            </div>
            <div class="col-md-6">
                <div class="pull-right">
                    <button id="backtosearch" type="button" class="btn btn-default">Back to Search</button>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 clearfix">
                <!-- Result -->
                <table id="table">
                    <thead>
                    <tr>
                        <th data-sortable="true" data-width="16%" data-field="name">Name of organization</th>
                        <th data-sortable="true" data-width="7%" data-field="acronym">Acronym</th>
