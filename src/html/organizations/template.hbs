                        <th data-sortable="true" data-width="21%" data-field="parentorg">Parent organization</th>
                        <th data-sortable="true" data-width="21%" data-field="address">Address</th>
                        <th data-sortable="true" data-width="14%" data-field="city">City</th>
                        <th data-sortable="true" data-width="14%" data-field="country">Country</th>
                    </tr>
                    </thead>
                </table>
            </div>
        </div>
        <div class="row spacer"><!-- Spacer --></div>
    </div>
