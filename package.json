{
  "name": "wiews-ui",
  "version": "1.0.1",
  "description": "Repo for Wiews",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "echo \"Error: please specify a valid section to develop [e.g. npm run dev:analysis]\" && exit 1",
    "dev:base": "cross-env SECTIONS=base NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:test": "cross-env SECTIONS=test NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:countryProfile": "cross-env SECTIONS=countryProfile NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:cultivars": "cross-env SECTIONS=cultivars NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:domains": "cross-env SECTIONS=domains NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:organizations": "cross-env SECTIONS=organizations NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "dev:sdg": "cross-env SECTIONS=sdg NODE_ENV=develop webpack-dev-server --content-base dev --display-error-details --progress --color --inline --hot",
    "build": "cross-env SECTIONS=base,countryProfile,cultivars,domains,organizations,sdg NODE_ENV=production webpack --display-error-details ",
    "build:base": "cross-env SECTIONS=base NODE_ENV=production webpack --display-error-details ",
    "build:countryProfile": "cross-env SECTIONS=countryProfile NODE_ENV=production webpack --display-error-details ",
    "build:cultivars": "cross-env SECTIONS=cultivars NODE_ENV=production webpack --display-error-details ",
    "build:domains": "cross-env SECTIONS=domains NODE_ENV=production webpack --display-error-details ",
    "build:organizations": "cross-env SECTIONS=organizations NODE_ENV=production webpack --display-error-details ",
    "build:sdg": "cross-env SECTIONS=sdg NODE_ENV=production webpack --display-error-details ",
    "build:stats": "cross-env SECTIONS=base,countryProfile,cultivars,domains,organizations,sdg NODE_ENV=production webpack --display-error-details --profile --json  > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FENIX-Platform-Projects/wiews-ui.git"
  },
  "author": "FENIX team (http://fenix.fao.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FENIX-Platform-Projects/wiews/issues"
  },
  "dependencies": {
    "amplify-pubsub": "^0.0.1",
    "bootstrap": "^3.3.7",
    "bootstrap-table": "^1.11.0",
    "d3": "^4.4.0",
    "fenix-ui-bridge": "^1.0.0-beta.18",
    "fenix-ui-reports": "^2.0.0-beta.5",
    "fenix-ui-dashboard": "^3.0.0-beta.30",
    "fenix-ui-dropdown": "^1.0.0-beta.3",
    "fenix-ui-filter": "^3.0.0-beta.85",
    "fenix-ui-filter-utils": "^1.0.0-beta.18",
    "highcharts": "^5.0.6",
    "highcharts-exporting": "^0.1.2",
    "highcharts-no-data-to-display": "^0.1.2",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "jstree": "^3.3.3",
    "leaflet": "^0.7.7",
    "leaflet-panel-layers": "^0.9.5",
    "loglevel": "^1.4.1",
    "moment": "^2.17.1",
    "swiper": "^3.4.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
